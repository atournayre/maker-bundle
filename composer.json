{
    "name": "atournayre/maker-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "This bundle Extend Symfony Maker Bundle.",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "atournayre/collection": "^0.4.0",
        "atournayre/types": "^0.7.0",
        "nette/php-generator": "^4.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/maker-bundle": "^1.56",
        "symfony/mime": "^6.4",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0",
        "webmozart/assert": "^1.11"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Atournayre\\Bundle\\MakerBundle\\": "src/",
            "App\\": "src/Resources/templates"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\Bundle\\MakerBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
