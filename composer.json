{
    "name": "atournayre/maker-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "This bundle Extend Symfony Maker Bundle.",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/maker-bundle": "^1.56",
        "symfony/twig-bundle": "^6.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Atournayre\\MakerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\MakerBundle\\Tests\\": "tests/"
        }
    }
}
