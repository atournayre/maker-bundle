{
    "description": "Atournayre Maker.",
    "name": "atournayre/maker-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "Aur√©lien Tournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "symfony/maker-bundle": "3.x-dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3|^5.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Atournayre\\Bundle\\MakerBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atournayre\\Bundle\\MakerBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
