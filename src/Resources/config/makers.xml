<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

        <services>
            <defaults public="false" />

            <service id="atournayre.maker.make_trait" class="Atournayre\Bundle\MakerBundle\Command\MakeTrait">
                <argument type="service" id="atournayre.maker.trait_generator" />
                <argument type="service" id="atournayre.maker.trait_for_collection_generator" />
                <argument type="service" id="atournayre.maker.class_trait_adder" />
                <tag name="maker.command" />
            </service>

        </services>
</container>
